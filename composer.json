{
    "name": "kanagama/laracakeorm",
    "description": "Running CakePHP's ORM on Laravel",
    "keywords": [
        "Laravel",
        "CakePHP",
        "ORM"
    ],
    "authors": [
        {
            "name": "kazuma nagama",
            "email": "k.nagama0632@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.0 | ^9.0 | ^10.0",
        "cakephp/cache": "^4.0",
        "cakephp/orm": "^4.0",
        "cakephp/i18n": "^4.0",
        "php": "^7.4 | ^8.0 | ^8.1 | ^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 | ^10.0",
        "orchestra/testbench": "*",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Kanagama\\LaraCakeORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "Kanagama\\LaraCakeORM\\Tests\\": "tests/"
        }
    },
    "exclude": [
        "tests/",
        ".docker/",
        ".github/",
        "*.md"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kanagama\\LaraCakeORM\\LaraCakeOrmProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}